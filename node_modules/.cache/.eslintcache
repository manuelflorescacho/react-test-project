<Col xs={1}>{leftSelector()}</Col>\n          <Col>\n            <Row style={{justifyContent: 'center'}}>\n              <Col xs={4} style={{justifyContent: 'center'}}>\n                <img src={svgProfile} style={myLogoStyle} />\n              </Col>\n            </Row>\n          </Col>\n          <Col xs={1}>{rightSelector()}</Col>\n        </Row>\n        <Row style={{justifyContent: 'center'}}>\n          <Col xs={4}>\n            <Routes>\n              {(() => {\n                let routes = []\n                for(var i=0; i < inComponents.length; i++) {\n                  routes.push(\n                    <Route\n                      key={inComponents[i].index}\n                      path={inComponents[i].path} \n                      element= {inComponents[i].component} \n                    />\n                  )\n                }\n                return routes;\n              })()}\n            </Routes>\n          </Col>\n        </Row>\n      </Card.Body>\n    </Card>\n  );\n}\n\nexport default PageCardHolder;","/Users/detmaneh/Development/react/react-test-project/src/components/AppContext/AppContext.js",[],[],{"ruleId":"74","severity":1,"message":"75","line":3,"column":27,"nodeType":"76","messageId":"77","endLine":3,"endColumn":33},{"ruleId":"74","severity":1,"message":"78","line":4,"column":8,"nodeType":"76","messageId":"77","endLine":4,"endColumn":16},{"ruleId":"74","severity":1,"message":"79","line":1,"column":8,"nodeType":"76","messageId":"77","endLine":1,"endColumn":12},{"ruleId":"74","severity":1,"message":"80","line":3,"column":27,"nodeType":"76","messageId":"77","endLine":3,"endColumn":36},{"ruleId":"74","severity":1,"message":"81","line":3,"column":38,"nodeType":"76","messageId":"77","endLine":3,"endColumn":48},{"ruleId":"74","severity":1,"message":"82","line":6,"column":5,"nodeType":"76","messageId":"77","endLine":6,"endColumn":11},{"ruleId":"74","severity":1,"message":"83","line":7,"column":5,"nodeType":"76","messageId":"77","endLine":7